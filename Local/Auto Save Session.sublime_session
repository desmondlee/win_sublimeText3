[\"loan_count\"=>1,\"repayment_count\"=>0],\"date\"=>‘’]；\n    }\n     //每日催回订单统计：(初始化)(可多次重复执行用于修复数据，本脚本会新增、更新所有记录)\n    public static function actionInit_1(){\n        ini_set('memory_limit', -1);\n        //历史每天的逾期催回率：(统计不同逾期天数的催收成功订单额)\n        //新的催回率计算：\n        \n        try{\n            $transaction= Yii::$app->db_assist->beginTransaction();//创建事务\n            $day_today =  strtotime(date('Y-m-d'),time());\n            $day_start = $day_today - 3600 * 24 * 91;//更新91天内数据\n\n            // $day_start = strtotime(date('Y-10-16'));//测试\n            // $day_today =  strtotime(date('Y-10-17'),time());//测试\n            for ($day=$day_start; $day < $day_today; $day+=3600*24) {\n                echo '催回率更新，更新日期：'.date(\"Y-m-d\", $day).\"\\r\\n\";\n\n                $repaymentIds = LoanCollectionOrder::repaymentIds_between_dateTime($day, $day+86400);\n                if(empty($repaymentIds))    continue;\n                $res = UserLoanOrderRepaymentService::principal_overduedays_in_ids($repaymentIds);\n                if(!empty($res)){\n                    $rate = array('create_at'=>$day,\n                                  'repay_1_amount'=>['old'=>0, 'new'=>0],\n                                  'repay_2_amount'=>['old'=>0, 'new'=>0],\n                                  'repay_3_amount'=>['old'=>0, 'new'=>0],\n                                  'repay_4_amount'=>['old'=>0, 'new'=>0],\n                                  'repay_5_amount'=>['old'=>0, 'new'=>0],\n                                  'repay_6_amount'=>['old'=>0, 'new'=>0],\n                                  'repay_7_amount'=>['old'=>0, 'new'=>0],\n                                  'repay_10_amount'=>['old'=>0, 'new'=>0],\n                                  'repay_30_amount'=>['old'=>0, 'new'=>0],\n                                  'repay_60_amount'=>['old'=>0, 'new'=>0],\n                                  'repay_90_amount'=>['old'=>0, 'new'=>0],\n                                  'repay_999_amount'=>['old'=>0, 'new'=>0],\n                                );\n                    foreach ($res as $key => $item) {\n                        $isOldPerson = UserLoanOrderRepaymentService::amount_condition(\"user_id = \".$item['user_id'].\" AND id < \".$item['id']);\n                        $type = $isOldPerson ? 'old' : 'new';\n                        // if($isOldPerson){\n                        //     echo 'user_id:'.$item['user_id'].\", id:\".$item['id'].\"\\r\\n\";exit;\n                        // }\n                        if($item['OverdueDays'] <=1){\n                            //逾期0,1天\n                            $rate['repay_1_amount'][$type] += (int)$item['total'] ??0;\n\n                        }elseif ($item['OverdueDays'] <=2) {\n                            //逾期2天\n                            $rate['repay_2_amount'][$type] += (int)$item['total'] ??0;\n\n                        }elseif ($item['OverdueDays'] <=3) {\n                            //逾期3天\n                            $rate['repay_3_amount'][$type] += (int)$item['total'] ??0;\n\n                        }elseif ($item['OverdueDays'] <=4) {\n                            //逾期4天\n                            $rate['repay_4_amount'][$type] += (int)$item['total'] ??0;\n\n                        }elseif ($item['OverdueDays'] <=5) {\n                            //逾期5天\n                            $rate['repay_5_amount'][$type] += (int)$item['total'] ??0;\n\n                        }elseif ($item['OverdueDays'] <=6) {\n                            //逾期6天\n                            $rate['repay_6_amount'][$type] += (int)$item['total'] ??0;\n\n                        }elseif ($item['OverdueDays'] <=7) {\n                            //逾期7天\n                            $rate['repay_7_amount'][$type] += (int)$item['total'] ??0;\n\n                        }elseif ($item['OverdueDays'] <=10) {\n                            //逾期8~10天\n                            $rate['repay_10_amount'][$type] += (int)$item['total'] ??0;\n\n                        }elseif ($item['OverdueDays'] <=30) {\n                            //逾期11~30天\n                            $rate['repay_30_amount'][$type] += (int)$item['total'] ??0;\n\n                        }elseif ($item['OverdueDays'] <=60) {\n                            //逾期31~60天\n                            $rate['repay_60_amount'][$type] += (int)$item['total'] ??0;\n\n                        }elseif ($item['OverdueDays'] <=90) {\n                            //逾期61~90天\n                            $rate['repay_90_amount'][$type] += (int)$item['total'] ??0;\n\n                        }else{\n                            //逾期91天以上\n                            $rate['repay_999_amount'][$type] += (int)$item['total'] ??0;\n\n                        }\n                    }\n                    // exit;\n                    // $rate['create_at'] = date('Y-m-d H:i:s', $rate['create_at']);\n                    // print_r($rate);exit;\n                    OrderStatisticsByRate::rate_amount($rate);\n                }\n\n            }\n\n            $transaction->commit();\n        }catch(\\Exception $e){\n            $transaction->rollBack();\n            Yii::error($e->getMessage(), 'collection');\n            echo $e->getMessage();\n\n        }\n        \n        // 初始化每天到期应还本金：\n        try{\n            // $transaction= Yii::$app->db_assist->beginTransaction();//创建事务\n            $time_end = date(\"Y-m-d\");\n            \n            $time_start = date(\"Y-m-d\", strtotime(\"-91 day\"));\n            // $time_start = date(\"Y-m-d\", strtotime(\"-3 day\"));\n\n            for ($day=$time_start; $day < $time_end; $day = date(\"Y-m-d\", strtotime($day.\"+1 day\"))) { \n                $res = UserLoanOrderRepaymentService::principal_planFeeTime_between_2($day);\n                // print_r($res);exit;\n                // $i = 1;\n                foreach ($res as $plan_feeDay => $item) {\n                    echo \"初始化每天到期应还本金:\".$day.'/'.$time_end.\"\\r\\n\";\n                   OrderStatisticsByRate::rate_amount(array('deadline_amount'=>$item,'create_at'=>strtotime($plan_feeDay)));//create_at 是入催时间\n                }\n            }\n            \n            // $transaction->commit();\n        }catch(\\Exception $e){\n            // $transaction->rollBack();\n            Yii::error($e->getMessage(), 'collection');\n            echo $e->getMessage();\n            exit;\n\n        }\n       \n        //初始化每天入催订单总本金， 入催订单量：\n        try{\n            // $transaction= Yii::$app->db_assist->beginTransaction();//创建事务\n            //获取所有入催时间：(执行一次即可更新所有数据)\n\n            $dates = LoanCollectionOrder::cui_dates();\n            //获取今天入催时间：(每天执行一次，即可更新当天数据)\n            // $dates = array(array('createTime'=>'2017-02-25'));\n\n            $i = 1;\n            $amount = count($dates);\n            foreach ($dates as $key => $day) {\n                echo \"初始化每天入催订单总本金, 入催订单量:\".$i++.'/'.$amount.\"\\r\\n\";\n                $result = LoanCollectionOrder::id_userid_between_dateTime($day['createTime'], $day['createTime']);\n                $res1 = UserLoanOrderRepaymentService::statistics_in_ids_2($result['user_loan_order_repayment_id'],$result['user_id'],  $day['createTime']);\n                // print_r($res1['old']+$res1['new']);exit;\n\n                OrderStatisticsByRate::rate_amount(array('collection_amount'=>$res1,'create_at'=>strtotime(date('Y-m-d 0:0:0', strtotime($day['createTime'])))));\n\n                $res = UserLoanOrderRepaymentService::statistics_in_ids($result['user_loan_order_repayment_id'], $day['createTime']);\n                OrderStatisticsByDay::collection_input_statistics(array('new_principal'=>$res[0]['Principal'],'new_amount'=>$res[0]['amount'],'create_at'=>strtotime(date('Y-m-d 0:0:0', strtotime($day['createTime'])))));\n            }\n\n            // $transaction->commit();\n        }catch(\\Exception $e){\n            // $transaction->rollBack();\n            Yii::error($e->getMessage(), 'collection');\n            echo $e->getMessage();\n\n        }\n       \n\n        //初始化每天新增还款单数，本金，滞纳金：\n        try{\n            $transaction= Yii::$app->db_assist->beginTransaction();//创建事务\n            //获取所有入催时间：(执行一次即可更新所有数据)\n\n            $dates = LoanCollectionOrder::cui_dates();\n\n            //获取今天入催时间：(每天执行一次，即可更新当天数据)\n            // $dates = array(array('createTime'=>date('Y-m-d')));\n\n            $i = 1;\n            $amount = count($dates);\n            foreach ($dates as $key => $day) {\n                echo '更新每天新增还款单数，本金，滞纳金：'.$i++.'/'.$amount.\"(\".$day['createTime'].\")\\r\\n\";\n\n                $result = UserLoanOrderRepaymentService::daily_detail($day['createTime']);\n                $validIds = array_column($result, 'id');\n                $validRepayIds = LoanCollectionOrder::get_valid_repayIds($validIds);\n\n                $res = array('Principal'=>0, 'amount'=>0, 'late_fee'=>0, 'create_at'=>0);\n                if(!empty($validRepayIds)){\n                    foreach ($validRepayIds as $key => $index) {\n                        $res['Principal'] += $result[$index]['Principal'];\n                        $res['amount'] ++;\n                        $res['late_fee'] += $result[$index]['late_fee'];\n\n                    }\n                }\n\n                OrderStatisticsByDay::collection_input_statistics(array('repay_principal'=>$res['Principal'],'repay_amount'=>$res['amount'],'repay_late_fee'=>$res['late_fee'],'create_at'=>$day['createTime']));\n            }\n\n            $transaction->commit();\n        }catch(\\Exception $e){\n            $transaction->rollBack();\n            Yii::error($e->getMessage(), 'collection');\n            echo $e->getMessage();\n\n        }\n\n    }\n\n    /**\n     *将问题订单派单给所有S2成员\n     *问题订单：已逾期显示催收成功但未成功还款的问题订单\n     */\n    /*\n    public function actionSpecialOrderDispatch(){\n\n        try{\n            $transaction= Yii::$app->db->beginTransaction();//创建事务\n            //1.找出问题订单：（催收成功、未还款）\n            $sq