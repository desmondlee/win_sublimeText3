ispatch_day){\n        ini_set('memory_limit', '-1');\n        // $dispatch_day = '2017-01-01';//派单时间\n        //要统计的机构:\n        // $outside_lists = UserCompany::getAll();\n        // foreach ($outside_lists as $key => $value) {\n        //     $dest_outside = $value['id'];\n           \n\n            // $limit = 1000;\n            $res = array();\n            $notice = array();//逾期天数不大于0的订单\n\n            // $outside = UserCompany::id($dest_outside);\n\n            //获取当天派单一次的：\n            $sql = \"SELECT `loan_collection_order_id`, `remark` FROM (select `loan_collection_order_id`, `remark` from `tb_loan_collection_status_change_log` WHERE `type` = \".LoanCollectionOrder::TYPE_DISPATCH_COLLECTION.\" AND `created_at` > \".strtotime($dispatch_day).\" AND `created_at` <= \".strtotime($dispatch_day.\" 23:59:59\").\"  order by id DESC)as a  group by `loan_collection_order_id` having count(`loan_collection_order_id`) =1 \";\n\n            $list = Yii::$app->db_assist->createCommand($sql)->queryAll();\n            $amount = count($list);\n            $i = 0;\n            foreach ($list as $key => $item) {\n                echo '进度：'.(++$i).'/'.$amount.\"\\r\\n\";\n                $remark = $item['remark'];\n                preg_match('/^[^\\d]+([\\d]+)[^\\d]+([\\d]+)$/',$remark,$match);\n                $collectionOrderId = $match[1];\n                $collectionUserId = $match[2];\n\n                \n                $loanCollection = LoanCollection::admin_id($collectionUserId);\n                // if(empty($loanCollection) || $loanCollection['outside'] != $dest_outside){\n\n                //     continue;\n                // }\n                // $loanOrder = LoanCollectionOrder::id($collectionOrderId);\n                $real_overdueDay = LoanCollectionOrder::get_real_overdueDay($collectionOrderId, $dispatch_day);\n                $real_overdueDay += 1;//校正逾期天数\n                if($real_overdueDay < 1){\n                    // echo '逾期天数不大于0collectionId:'.$collectionOrderId.\"\\r\\n\";\n                    $notice[] = $collectionOrderId;\n                    continue;\n                    // exit;\n                }\n                $userGroup = $loanCollection['group'];\n                isset($res[$loanCollection['outside']][$userGroup][$real_overdueDay]['amount']) ? ($res[$loanCollection['outside']][$userGroup][$real_overdueDay]['amount']+=1) : ($res[$loanCollection['outside']][$userGroup][$real_overdueDay]['amount'] = 1);\n\n                $loanCollectionOrder = LoanCollectionOrder::id($collectionOrderId);\n                if($loanCollectionOrder['status'] == LoanCollectionOrder::STATUS_COLLECTION_FINISH && $LoanCollectionOrder['current_collection_admin_user_id'] == $collectionUserId){\n                    $info = &$res[$loanCollection['outside']][$userGroup][$real_overdueDay]['success'];\n                    isset($info) ? ($info += 1) : ($info = 1);\n                }\n                \n            }\n\n          \n               \n            \n            if (!empty($res)) {\n                \n                // $total = array('amount'=>0, 'success'=>0);\n                // foreach ($res as $outside => &$value) {\n                   \n                //     foreach ($value as $group => &$values) {\n                //         foreach ($values as &$each) {\n                //             if(!isset($each['amount'])) $each['amount'] = 0;\n                //             if(!isset($each['success'])) $each['success'] = 0;\n                //             $total['amount'] += $each['amount'];\n                //             $total['success'] += $each['success'];\n                //         }\n                //     }\n                // }\n                // $html = \"<table style='border:1px solid black;'><tr><th>催收分组</th><th>逾期天数</th><th>派单数量</th><th>派单占比</th><th>催收成功数量</th><th>催收成功占比</th><th>催回率</th></tr>\";\n                \n                foreach ($res as $outside => $value) {\n                    foreach ($value as $groupId=> $item) {\n                       \n                        foreach ($item as $days => $each) {\n                            $data['dispatch_time'] = $dispatch_day;\n                            $data['outside'] = $outside;\n                            $data['group'] = $groupId;\n                            $data['overdue_day'] = $days;\n                            $data['dispatch_amount'] = $each['amount'];\n                            $data['dispatch_principal'] = 0;\n                            $data['back_principal'] = 0;\n                            $data['back_amount'] = isset($each['success']) ? $each['success'] : 0;\n                            // $html .= \"<tr><td>\".LoanCollection::group($groupId, true).\"</td><td>{$days}</td><td>{$each['amount']}</td><td>\".(round($each['amount']/$total['amount'], 4)*100).\"%</td><td style='text-align:center;'>{$each['success']}</td><td>\".(round($each['success']/$total['success'], 4)*100).\"%</td><td>\".(round($each['success']/$each['amount'], 4)*100).\"%</td></tr>\";\n                            StatisticsDispatchDay::new_record($data);\n\n                        }\n                    }\n                }\n               \n            }\n\n           \n        // }\n\n    }\n\n    /**\n     *查看某订单的旧还款数据\n     */\n    public function actionRepayorderOld($id){\n        $sql = \"SELECT id,order_id,user_id,plan_repayment_time,true_repayment_time,plan_fee_time FROM kdkj.tb_user_loan_order_repayment where  order_id = \".$id;\n        $ret = Yii::$app->db_kdkj_old_rd->createCommand($sql)->queryAll();\n        if(!empty($ret)){\n            foreach ($ret as $key => $item) {\n                $ret[$key]['plan_fee_time_format'] = date(\"Y-m-d H:i:s\", $item['plan_fee_time']);\n                $ret[$key]['true_repayment_time_format'] = date(\"Y-m-d H:i:s\", $item['true_repayment_time']);\n            }\n        }\n        print_r($ret);\n        // MailHelper::send('lizhenguo@xianjincard.com', date('Y-m-d H:i:s').\" 订单数据, orde