   $lists = Yii::$app->db_kdkj_rd->createCommand($sql)->queryAll();\n\n        LoanCollectionOrder::ru_cui($lists, '入催未曾入催的问题订单');\n    }\n\n\n\n    /**\n     *2016年12月14日\n     */\n    public function actionRucui1214(){\n        ini_set('memory_limit', '-1');\n        try{\n            $start = 0;\n            $limit = 1000;\n            $total = array();\n            $list = UserLoanOrderRepaymentService::unpayed($start*$limit, $limit);\n            $amount =  UserLoanOrderRepaymentService::unpayed_count();\n            while (!empty($list)) {\n                echo '处理进度：'.\" start:\".$start*$limit.\", limit:\".$limit.\", 总量：$amount\".\"\\r\\n\";\n\n                $repayIds = array_column($list, 'id');\n                $validRepayIds = LoanCollectionOrder::get_valid_repayIds($repayIds);\n                $diff = array_diff($repayIds, $validRepayIds);\n                if(!empty($diff)){\n                    $total = array_merge($total, $diff);\n                }\n                $start++;\n                $list = UserLoanOrderRepaymentService::unpayed($start*$limit, $limit);\n            }\n\n            $ru_cui = array();\n            if(!empty($total)){\n                $sql = \"SELECT a.id, a.user_id, a.order_id, a.plan_fee_time, a.`status`,a.is_overdue,a.overdue_day, b.order_type FROM tb_user_loan_order_repayment AS a, tb_user_loan_order AS b WHERE a.order_id = b.id AND  a.id IN(\".implode(',', $total).\") ORDER BY a.id DESC\";\n\n                $lists = Yii::$app->db_kdkj_rd->createCommand($sql)->queryAll();\n\n                $html = \"<table><thead><th>还款ID</th><th>用户ID</th><th>借款ID</th><th>应还时间</th><th>还款状态</th><th>是否逾期</th><th>逾期天数</th><th>订单类型</th></thead><tbody>\";\n                if(!empty($lists)){\n\n                    foreach ($lists as $key => $item) {\n                        if($item['overdue_day'] < 8){\n                            $ru_cui[] = $item;\n                        }\n                        $tr = \"<tr><td>{$item['id']}</td><td>{$item['user_id']}</td><td>{$item['order_id']}</td><td>\".date(\"Y-m-d\", $item['plan_fee_time']).\"</td><td>\".UserLoanOrderRepayment::$status[$item['status']].\"</td><td>\".(intval($item['is_overdue']) > 0 ? '是' : '否').\"</td><td>{$item['overdue_day']}</td><td>\".UserLoanOrder::$loan_type[$item['order_type']].\"</td></tr>\";\n                        $html .= $tr;\n                    }\n                }\n                $html.= \"</tbody></table>\";\n                MailHelper::send('lizhenguo@xianjincard.com', date('Y-m-d H:i:s').\"已逾期，但未存在于催收表中的数据\", $html);\n                if(!empty($ru_cui)){\n                    MailHelper::send('lizhenguo@xianjincard.com', date('Y-m-d H:i:s').\"已逾期，但未存在于催收表中的数据（执行入催）\", '入催数量：'.count($ru_cui));\n                    LoanCollectionOrder::ru_cui($ru_cui, '已逾期，但未存在于催收表中的数据');\n                }\n\n            }\n        }catch(Exception $e){\n            Yii::error($e->getMessage(), 'collection');\n        }\n    }\n\n\n\n    /**\n     *手动修改催单状态，用于修复数据\n     *@param int $collectionId 要修改的催单ID\n     *@param int $status 要修改成的催收状态\n     *@param string 状态变更原因\n     */\n    public function actionManualStatus($collectionId, $status, $remark){\n        $loanCollectionService = Yii::$container->get('loanCollectionService');\n        $res = $loanCollectionService->chstatus($collectionId, $status, $remark);\n        if(empty($res)){\n            echo \"\\r\\n>>>更新催收订单状态失败<<<\\r\\n\";\n        }\n    }\n\n    /**\n     *手动入催\n     *@param int $orderId 订单ID\n     */\n    public function actionManualInput($orderId, $remark){\n        $repayOrder = UserLoanOrderRepaymentService::order_id($orderId);\n        if(empty($repayOrder)){\n            echo '还款订单不存在'.\"\\r\\n\";\n        }else{\n\n            if($repayOrder['is_overdue'] == 1){\n                echo '执行入催，订单ID：'.$orderId.\"\\r\\n\";\n                LoanCollectionOrder::ru_cui(array($repayOrder), $remark);\n            }else{\n                echo '订单未逾期、不执行入催，订单ID：'.$orderId.\"\\r\\n\";\n                 print_r($repayOrder);\n            }\n        }\n\n\n    }\n\n    /**\n     *补发派单邮件\n     */\n    public function actionDispatchMail(){\n        try{\n               \n                $today = strtotime(date('Y-m-d'));\n                $result = LoanCollectionOrder::amount_dispatchTime_after($today);\n                $companies = UserCompany::lists();\n                // print_r($result);exit;\n                if(!empty($result)){\n                    // unset($result[0]);//去除没有分组的部分\n                    foreach ($result as $key => $item) {\n                        if($item['company_id'] == 0)    continue;\n                        $result[$key]['title'] = $companies[$item['company_id']]['title'];\n                    }\n                }\n                $body = \"以下是派单结果数据：<br/>\";\n                $total = 0;\n                $group_amount = array();\n                if(!empty($result)){\n                    foreach ($result as $key => $item) {\n                        if($item['company_id'] == 0)    continue;\n                        $_group_user = LoanCollection::group_user($item['group_id'], $item['company_id']);\n                        $_avg = empty($_group_user) ? 0 : ($item['amount'] / count($_group_user));\n                        $body .= (\"公司：\" . $item['title'] . \", 分组：\" . LoanCollection::$group[$item['group_id']] . \", 派单数量：\" . $item['amount'] .\" (人数：\".count($_group_user).\", 人均派单量：\".round($_avg, 1).\")<br/>\");\n                        $total += $item['amount'];\n                        isset($group_amount[LoanCollection::$group[$item['group_id']]]) ? ($group_amount[LoanCollection::$group[$item['group_id']]]+= $item['amount']) : ($group_amount[LoanCollection::$group[$item['group_id']]]=$item['amount']);\n                    }\n                }\n                $str = '';\n                if(!empty($group_amount)){\n                    foreach ($group_amount as $key => $value) {\n                        $str .= ($key .' : '.$value.\", \");\n                    }\n                }\n                $str = substr($str, 0, -2);\n                $body .= \"总派单量：\".$total.\" (各组派单量, \".$str.\")\";\n               MailHelper::send('lizhenguo@xianjincard.com', date('Y-m-d H:i:s').\" 派单结果\", $body);\n\n            }catch(\\Exception $e){\n                Yii::error($e->getMessage(),'collection');\n                echo $e->getMessage();\n            }\n    }\n\n\n    /**\n     *为催收建议数据表补充机构信息\n     *检查是否有未处理记录：SELECT * FROM tb_loan_collection_suggestion_change_log WHERE operator_name != '系统' AND outside = 0;\n     */\n    public function actionSuggestionOutside(){\n        $start = 0;\n        $limit = 1000;\n        $i = 0;\n        $lists = LoanCollectionSuggestionChangeLog::find()->orderBy(['id'=>SORT_ASC])->where(\"`operator_name` != '系统' AND `outside` = 0\")->offset($start*$limit)->limit($limit)->all();\n        $total = LoanCollectionSuggestionChangeLog::find()->select(\"count(id)\")->orderBy(['id'=>SORT_ASC])->where(\"`operator_name` != '系统' AND `outside` = 0\")->scalar();\n        while (!empty($lists)) {\n            $username = array_column($lists, 'operator_name');\n            // foreach ($lists as $key => $item) {\n            //     $username[] = $item['operator_name'];\n            // }\n            $users = LoanCollection::list_username($username);\n            if(!empty($lists)){\n                foreach ($lists as $key => $item) {\n                    echo '进度：'.(++$i).'/'.$total.\"\\r\\n\";\n\n                    $item->outside = $users[$item['operator_name']]['outside'];\n                    if(!$item->save()){\n                        echo '更新机构信息失败，username:'.$username.\"\\r\\n\";\n                        print_r($users[$item->operator_name]);\n                        exit;\n                    }\n                }\n            }\n\n            $start++;\n            $lists = LoanCollectionSuggestionChangeLog::find()->orderBy(['id'=>SORT_ASC])->where(\"`operator_name` != '系统' AND `outside` = 0\")->offset($start*$limit)->limit($limit)->all();\n        }\n    }\n\n   \n    /**\n     *手动更新催收反馈信息中的催收机构\n     *@param int $orderId 订单ID\n     */\n    public function actionUserFeedbackCollectionOutside(){\n        $type   = UserFeedback::TYPE_COLLECTION; // 催收反馈问题类型\n        $collection_user_feedback_list   = UserFeedbackService::getUserFeedListAll($type);\n        foreach ($collection_user_feedback_list as $key => $value) {\n            $collection_user_feedback_order_admin_list = mainCollection::actionUserFeedbackOrderGetAll($value['user_id'], $value['created_at']);\n            // 判断催收用户反馈表中该ID是否存在\n            $userFeedbackId     = UserFeedbackService::UserFeedback_id($value['id']);\n            if (empty($userFeedbackId)) {\n                echo 'Collect UserFeedback of user information does not exist'.\"\\r\\n\";\n            }else{\n                // 如果催收机构outside等于0执行更新信息\n                if ($userFeedbackId['outside'] == 0) {\n                    if (!empty($collection_user_feedback_order_admin_list)) {\n                        $outside            = $collection_user_feedback_order_admin_list[0]['outside'];\n                        $admin_user_id      = $collection_user_feedback_order_admin_list[0]['admin_user_id'];\n                        $user_loan_order_id = $collection_user_feedback_order_admin_list[0]['user_loan_order_id'];\n                        $status = UserFeedbackService::updateOutside($value['id'], $outside, $admin_user_id, $user_loan_order_id);\n                        if ($status) {\n                            echo 'Update feedback user table is successful,ID is :' . $value['id'].\"\\r\\n\";\n                        }\n                    }\n                }\n            }\n        }\n    }\n\n\n\n\n    /**\n     *将27号没有入催的订单（目前351单），群发一波提醒短信\n     */\n    // public function actionManual1230(){\n    //     ini_set('memory_limit', '-1');\n    //     $query = \"SELECT * FROM tb_user_loan_order_repayment WHERE is_over